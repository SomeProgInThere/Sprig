
func fact_for(num: int): int {
    var fact = 1
    for i in 1..num {
        fact = fact * i
    }
    return fact
}

func fact_while(num: int): int {
    var fact = 1
    var i = 1
    while i <= num {
        fact = fact * i
        i = i + 1
    }
    return fact
}

func main() {
    let x = 5
    print(fact_for(x))
    print(fact_while(x))
}